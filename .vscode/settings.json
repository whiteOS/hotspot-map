{
    // eslint + format
    "eslint.options": {
        "extensions": [
        ".js",
        ".jsx",
        ".ts",
        ".tsx",
        ".html",
        ".vue"
        ]
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "html",
        "vue"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    }
}